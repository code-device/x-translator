{
	"name": "x-translator",
	"publisher": "codexu",
	"displayName": "x-translator",
	"description": "一键翻译、命名规则、替换。",
	"keywords": [
		"translation",
		"variable",
		"select"
	],
	"repository": "https://github.com/codexu/x-translator",
	"icon": "images/logo.png",
	"version": "0.1.13",
	"engines": {
		"vscode": "^1.14.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.x-translator"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "x-translator",
			"properties": {
				"x-translator.type": {
					"type": "string",
					"enum": [
						"google",
						"youdao",
						"baidu"
					],
					"default": "youdao",
					"description": "选择翻译服务商，设置完请重新打开vscode。"
				}
			}
		},
		"commands": [
			{
				"command": "extension.x-translator",
				"title": "x-translator"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.x-translator",
					"when": "editorHasSelection",
					"group": "x-translator"
				}
			]
		},
		"keybindings": [
			{
				"command": "extension.x-translator",
				"key": "ctrl+shift+r",
				"mac": "cmd+shift+r",
				"when": "editorHasSelection"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "7.1.1",
		"@types/mocha": "5.2.6",
		"@types/node": "10.12.21",
		"@types/vscode": "1.14.0",
		"glob": "7.1.4",
		"mocha": "6.1.4",
		"tslint": "5.12.1",
		"typescript": "3.3.1",
		"vscode-test": "1.2.0"
	},
	"dependencies": {
		"@types/lodash": "4.14.144",
		"lodash": "4.17.21",
		"translation.js": "0.7.15"
	}
}
