{
	"name": "x-translator",
	"publisher": "codexu",
	"displayName": "x-translator",
	"description": "x-translator",
	"repository": "https://github.com/codexu/x-translator",
	"icon": "images/logo.png",
	"version": "0.0.10",
	"engines": {
		"vscode": "^1.14.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.XTranslator"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.XTranslator",
				"title": "x-translator"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.XTranslator",
					"when": "editorHasSelection",
					"group": "x-translator"
				}
			]
		},
		"keybindings": [
			{
				"command": "extension.XTranslator",
				"key": "ctrl+shift+r",
				"mac": "cmd+shift+r",
				"when": "editorHasSelection"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.14.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"translation.js": "^0.7.9"
	}
}
